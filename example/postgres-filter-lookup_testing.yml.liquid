exec:
  max_threads: 1
  min_output_tasks: 1
in:
  type: postgresql
  host: localhost
  port: 5432
  user: postgres
  password: 1234
  schema: public
  database: test
  table: customer
filters:
  - type: postgress_lookup
    host: localhost
    port: 5432
    database: test
    username: postgres
    password: 1234
    schema: public
    table_name: car
    mapping_from:
      - car_name
      - company
    mapping_to:
      - model
      - brand
    new_columns:
      - { name: car_id, type: string }
      - { name: category, type: string }
      - { name: fuel_capacity, type: string }
out:
  type: postgresql
  host: localhost
  port: 5432
  database: test
  user: postgres
  password: 1234
  schema: public
  table:  output
  mode: truncate_insert

